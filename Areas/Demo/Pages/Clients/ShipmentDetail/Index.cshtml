@page
@model RevisionTwo_App.Areas.Demo.Pages.Clients.ShipmentDetail.IndexModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ShipmentDetail_App[0].OrderType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ShipmentDetail_App[0].OrderNbr)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ShipmentDetail_App[0].InventoryID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ShipmentDetail_App[0].WarehouseID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ShipmentDetail_App[0].ShippedQty)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ShipmentDetail_App[0].OrderedQty)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ShipmentDetail_App[0].Description)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.ShipmentDetail_App) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrderType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OrderNbr)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InventoryID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WarehouseID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ShippedQty)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OrderedQty)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
